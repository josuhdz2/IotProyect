<!--carga de plantillas-->
<%-include('templates/init.ejs')%>
<div class="container text-center mt-5 pt-5">
    <h1>Iniciar sesion</h1>
    <!--creacion de formulario para la autenticacion e inicio de sesion-->
    <form action="/login" method="post">
        <input class="form-control mt-5 mb-3" type="email" name="email" placeholder="Correo Electronico" autocomplete="off">
        <input class="form-control mb-3" type="password" name="password" placeholder="Password" autocomplete="off" minlength="8">
        <button type="submit" class="btn btn-success mb-5">Iniciar sesion</button>  
    </form>
</div>
<%-include('templates/end.ejs')%>